{
  "name": "array-sqlizr",
  "version": "1.0.0",
  "description": "A typescript utility that allows you to work with arrays in a SQLish way.",
  "author": {
    "name": "dhlevi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhlevi/array-sqlizr.git"
  },
  "keywords": [
    "arrays",
    "utility",
    "typescript"
  ],
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run lint && rimraf dist/* && npm run build-ts && npm run test",
    "debug": "npm run build && npm run watch-debug",
    "test": "jest --forceExit --coverage --verbose",
    "lint": "tsc --noEmit && eslint \"**/*.ts\" --quiet --fix"
  },
  "dependencies": {
    "expression-eval": "^3.1.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-jest": "^26.6.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "rimraf": "3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "~4.1.3"
  },
  "license": "Apache-2.0"
}
